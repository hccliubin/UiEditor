<?xml version="1.0" encoding="utf-8"?>

<root>
	<控件列表>
		<!-- pos和size都不是必须的，设计器根据鼠标按下的位置设置pos -->

		<caption pos="0,0" size="300, 30" show="1" font="adding:0">
			<text pos="29,9">标题</text>
			<imgbtn name="btn_close" skin="_skin.sys.btn.close"    pos="-45,0" tip="close" animate="1"/>
			<imgbtn name="btn_max" skin="_skin.sys.btn.maximize"  pos="-83,0" animate="1" />
			<imgbtn name="btn_restore" skin="_skin.sys.btn.restore"  pos="-83,0" show="0" animate="1" />
			<imgbtn name="btn_min" skin="_skin.sys.btn.minimize" pos="-121,0" animate="1" />
		</caption>	
		<icon pos="0,0" src="LOGO:16"/>
		<text pos="0,0">文本</text>
		<edit pos="0,0" size="100,25"  colorBkgnd="#FFFFFF" cueText="" colorText="#000000"/>
		<button pos="0,0" size="90,30" colorText="#000000"
		skin="_skin.sys.btn.normal">按钮</button> 
		<imgbtn pos="0,0" size="90,30"  tip="" animate="1"/>

		<toolbox pos="0,0" size="120,30"  skin="" icon=""
                 text="toolbox" animate="1"
                 class=""/>		
		<radio pos="0,0" size="90,30" skin="_skin.sys.radio">单选框</radio>  	


		<check pos="0,0" size="90,30" skin="_skin.sys.checkbox">复选框</check>  	

		<group pos="0,0" size="100,100" align="left">组合框</group>

		<window pos="0,0" size="100,100">	

		</window>	
		
		<realwnd pos="0,0" size="100,100" name="" wndclass="" id="" wndname="" />
		
		<include src=""/>

		<listbox pos="0,0" size="200,200" hotTrack="1"   itemHeight="30" colorText="#FFFFFF" colorSelText="#FFFFFF" colorItemSelBkgnd="rgb(234,128,16)" text-x="20">
			<items>
				<item text="item1" icon="1" data="100" />
				<item text="item2" icon="2" data="100" />
				<item text="item3" icon="3" data="103" />
				<item text="item4" icon="4" data="100" />
				<item text="item5" icon="5" data="105" />
				<item text="item6" icon="0" data="100" />
			</items>
		</listbox>

		<tabctrl  pos="0,0" size="200,200"  tabAlign="left" tabSkin="" tabInterSize="2" tabWidth="70" tabHeight="30" colorText="#FFFFFF" focusable="0" show="1" curSel="0" >

			<page title="页1" tip="提示1">
			</page>
			<page title="页2" tip="提示2">
			</page>
		</tabctrl>  	


	</控件列表>



	<容器控件>
		<!--可以在上面摆放其他控件的控件，需要再这里声明，否则上面不能摆放控件-->
		<caption/>
		<group/>
		<window/>
		<page/>
	</容器控件>
</root>	

